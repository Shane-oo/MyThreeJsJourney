    "@angular/platform-browser": "~15.0.2",
    "@angular/platform-browser-dynamic": "~15.0.2",
    "@angular/platform-server": "~15.0.2",
    "@angular/router": "~15.0.2",
    "bootstrap": "^5.1.0",
    "cannon": "^0.6.2",
    "dat.gui": "^0.7.9",
    "glslify-loader": "^2.0.0",
    "gsap": "^3.10.4",
    "jquery": "^3.5.1",
    "lil-gui": "^0.16.1",
    "oidc-client": "^1.11.5",
    "popper.js": "^1.16.0",
